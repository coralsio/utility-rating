{
    "name": "corals/utility-rating",
    "description": "Utility Rating",
    "keywords": [
        "corals",
        "utility",
        "rating"
    ],
    "homepage": "https://github.com/coralsio/utility-rating",
    "license": "MIT",
    "authors": [
        {
            "name": "corals",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2"

    },
    "autoload": {
        "psr-4": {
            "Corals\\UtilityRating\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Corals\\UtilityRating\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Corals\\UtilityRating\\UtilityRatingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
